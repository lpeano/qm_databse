MAKE=make
include ./make.mk

testlogger: 
	@echo ""
	@echo "**********************************************************************************************************"
	@echo "****************************************Starting making $@"
	@echo "**********************************************************************************************************"
	$(mk_all_module)
	$(mk_all)
	$(install)
	mkdir -p $(base_path)/bin
	cp test/$@  $(base_path)/bin
